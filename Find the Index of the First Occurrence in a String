int x=-1;
        int y=haystack.length()-needle.length()+1;
        for(int i=0;i<y+;i++){
            for(int j=0;j<needle.length(); j++){
            if(haystack.charAt(i+j)!=needle.charAt(j)){
                   break;
            }
              if(j==(needle.length()-1)){
                  return i;
              }
        }

        }
  return x;
